{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "buildCommand": "npm run vercel-build"
            }
        }
    ],
    "routes": [
        { "src": "/data/(.*)", "dest": "/data/$1" },
        { "src": "/(.*)", "dest": "/index.html" }
    ]
}
