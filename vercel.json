{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "routes": [
        { "src": "/assets/(.*)", "dest": "/assets/$1" },
        { "src": "/data/(.*)", "dest": "/data/$1" },
        { "src": "/bundle.js", "dest": "/bundle.js" },
        { "src": "/(.*)", "dest": "/index.html" }
    ]
}
