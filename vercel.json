{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "."
            }
        }
    ],
    "routes": [
        { "src": "/assets/(.*)", "dest": "/$1" },
        { "src": "/data/(.*)", "dest": "/data/$1" },
        { "src": "/bundle.js", "dest": "/bundle.js" },
        { "src": "/(.*)", "dest": "/index.html" }
    ]
}
